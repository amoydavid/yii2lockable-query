{
    "name": "amoydavid/yii2lockable-query",
    "description": "MySQL InnoDB lock support for ActiveRecord",
    "type": "yii-extension",
    "require": {
        "yiisoft/yii2": "~2.0",
        "php": "~7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wei Liu",
            "email": "liuw@liuw.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "amoydavid\\Yii2LockableQuery\\": "src/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
